{
  "name": "geekplus-expand",
  "displayName": "geekplus-expand",
  "description": "极智嘉vscode前端综合拓展",
  "keywords": [
    "geekplus",
    "expand"
  ],
  "icon": "static/icon.png",
  "publisher": "geekArk",
  "repository": "https://github.com/ChinaGeekPlus/geekplus-expand.git",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Extension Packs"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "geekplus"
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "geek-vue-i18n-ally",
          "title": "前端语言处理工具 - GEEK +",
          "icon": "./static/menu-icon.svg"
        }
      ]
    },
    "views": {},
    "commands": [
      {
        "command": "geekplus_command.toolWebView",
        "title": "open geekPlus expand",
        "icon": {
          "light": "static/light/icon.svg",
          "dark": "static/dark/icon.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "geekplus_command.readytoolWebView && editorFocus",
          "command": "geekplus_command.toolWebView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.2.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "glob": "^7.2.0",
    "simple-git": "^2.47.0"
  }
}
